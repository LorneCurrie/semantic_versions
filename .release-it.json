{
  "verbose": true,
  "non-interactive": true,
  "prereleaseId": "rc",
  "changelogCommand": "git log --pretty=format:\"* %s (%h)\" [REV_RANGE]",
  "src": {
    "commitMessage": "Release %s",
    "tagAnnotation": "Release %s"
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false,
    "publishPath": ".",
    "tag": "latest",
    "private": false,
    "access": null
  },
  "prompt": {
    "src": {
      "status": "Y",
      "commit": "Y",
      "tag": "Y",
      "push": "Y",
      "release": "Y",
      "publish": "N"
    }
  }
}
